                "value": 1
              }
            }
          ],
          "to_if_alone": [
            [
              {
                "key_code": "keypad_period"
              }
            ]
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "press_period_key",
                  "value": 0
                }
              }
            ],
            "to_if_canceled": [
              {
                "set_variable": {
                  "name": "press_period_key",
                  "value": 0
                }
              }
            ]
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "press_period_key",
              "value": 0
            }
          ]
        }
      ]
    },
