                "^com\\.p5sys\\.jump\\.mac\\.viewer\\.web$",
                "^com\\.vmware\\.horizon$",
                "^com\\.2X\\.Client\\.Mac$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f10",
            "modifiers": {
              "mandatory": [
                "shift"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "pointing_button": "button2"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
