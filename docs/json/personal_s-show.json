{
  "title": "Personal rules (@s-show)",
  "rule_list": false,
  "make_extra_description": true,
  "rules": [
    {
      "description": "command-E Open Finder",
      "extra_descriptions": [
        "It achieves the same key binding as Windows",
        "Windowsと同じキーバインドを実現"
      ],
      "extra_description_level": 1,
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'finder'"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.microsoft\\.rdc$",
                "^com\\.microsoft\\.rdc\\.mac$",
                "^com\\.microsoft\\.rdc\\.osx\\.beta$",
                "^net\\.sf\\.cord$",
                "^com\\.thinomenon\\.RemoteDesktopConnection$",
                "^com\\.itap-mobile\\.qmote$",
                "^com\\.nulana\\.remotixmac$",
                "^com\\.p5sys\\.jump\\.mac\\.viewer$",
                "^com\\.p5sys\\.jump\\.mac\\.viewer\\.web$",
                "^com\\.vmware\\.horizon$",
                "^com\\.2X\\.Client\\.Mac$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "control-Esc Open LaunchPad",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "escape",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "vk_launchpad"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.microsoft\\.rdc$",
                "^com\\.microsoft\\.rdc\\.mac$",
                "^com\\.microsoft\\.rdc\\.osx\\.beta$",
                "^net\\.sf\\.cord$",
                "^com\\.thinomenon\\.RemoteDesktopConnection$",
                "^com\\.itap-mobile\\.qmote$",
                "^com\\.nulana\\.remotixmac$",
                "^com\\.p5sys\\.jump\\.mac\\.viewer$",
                "^com\\.p5sys\\.jump\\.mac\\.viewer\\.web$",
                "^com\\.vmware\\.horizon$",
                "^com\\.2X\\.Client\\.Mac$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "If open 'Alfred' at IME ON, IME OFF before open Alfred",
