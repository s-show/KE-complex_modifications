    {
      "description": "(enthumble) 無変換/英数 + 「A,E」で「Ctrl-A, Ctrl-E」",
      "extra_descriptions": [
        "無変換/英数 + A,E -> Ctrl-A, Ctrl-E"
      ],
      "extra_description_level": 1,
      "manipulators":
      <%=
        manipulators = each_key(
