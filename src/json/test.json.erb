{
  "title": "TEST!",
  "rules": [
    {
      "description": "test to_delayed_action",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["control"]) %>,
          "to": [
            {
              "set_variable": {
                "name": "ctrl-a",
                "value": 1
              }
            }
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "ctrl-a",
                  "value": 2
                }
              }
            ],
            "to_if_canceled": [
              {
                "set_variable": {
                  "name": "ctrl-a",
                  "value": 0
                }
              }
            ]
          }
        }
      ]
    },
    {
      "description": "any key_code",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("any") %>,
          "to": <%= to(["a"]) %>
        }
      ]
    },
    {
      "description": "ctrl-aキーの無効化（CotEditor限定）",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["control"]) %>,
          "conditions": [
            <%= frontmost_application_if("coteditor") %>
          ]
        }
      ]
    },
    {
      "description": "test lazy",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_command") %>,
          "to": [
            {
              "key_code": "left_control",
              "lazy": true
            }
          ]
        },
        {
          "type": "basic",
          "from": <%= from("9") %>,
          "to": <%= to(["q"]) %>
        }
      ]
    },
    {
      "description": "test Cmd Holding",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_command") %>,
          "to": <%= to(["japanese_kana"]) %>
        },
        {
          "type": "basic",
          "from": <%= from("left_command") %>,
          "to_if_held_down": <%= to(["left_command"]) %>
        }
      ]
    },
    {
      "description": "test modifier 'A'",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("b", ["a"]) %>,
          "to": [
            {
              "key_code": "left_arrow",
              "lazy": true
            }
          ]
        }
      ]
    },
    {
      "description": "Cmd to Ctrl",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_command") %>,
          "to": <%= to(["right_control"]) %>,
          "conditions": [
            <%= frontmost_application_unless("safari") %>
          ]
        },
        {
          "type": "basic",
          "from": <%= from("left_command") %>,
          "to": <%= to(["left_control"]) %>,
          "conditions": [
            <%= frontmost_application_unless("safari") %>
          ]
        }
      ]
    },
    {
      "description": "エスペラント語入力のテスト",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("q") %>,
          "to": <%= to([
            ["6", ["option"]],
            ["s"]
            ]) %>
        },
        {
          "type": "basic",
          "from": <%= from("w") %>,
          "to": <%= to([
            ["6", ["option"]],
            ["g"]
            ]) %>
        },
        {
          "type": "basic",
          "from": <%= from("y") %>,
          "to": <%= to([
            ["6", ["option"]],
            ["u"]
            ]) %>
        },
        {
          "type": "basic",
          "from": <%= from("x") %>,
          "to": <%= to([
            ["6", ["option"]],
            ["c"]
            ]) %>
        },
        {
          "type": "basic",
          "from": <%= from("w") %>,
          "to": <%= to([
            ["6", ["option"]],
            ["g"]
            ]) %>
        },
        {
          "type": "basic",
          "from": <%= from("w") %>,
          "to": <%= to([
            ["6", ["option"]],
            ["g"]
            ]) %>
        }
      ]
    },
    {
      "description": "テンキー上のピリオド２回でコンマ入力",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("keypad_period") %>,
          "to": [
            <%# 下の"to"で入力された"keypad_period"を削除して"comma"を入力する %>
            { "key_code": "delete_or_backspace" },
            { "key_code": "comma" },
            <%= set_variable(["press_period_key"], [0]) %>
          ],
          "conditions": [
            <%= variable_if(["press_period_key"], [1]) %>
          ]
        },
        {
          "type": "basic",
          "from": <%= from("keypad_period") %>,
          "to": [
            <%= set_variable(["press_period_key"], [1]) %>,
            { "key_code": "keypad_period"}
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              <%= set_variable(["press_period_key"], [0]) %>
            ],
            "to_if_canceled": [
              <%= set_variable(["press_period_key"], [0]) %>
            ]
          },
          "conditions": [
            <%= variable_if(["press_period_key"], [0]) %>
          ]
        }
      ]
    },
    {
      "description": "Double tap 'left_control'",
      <%# Gistにも掲載中 https://gist.github.com/s-show/4afc5ca6a00d39a87a762fd4d4a7249b %>
      "comment": "この設定は期待どおりの動作になる。",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_control") %>,
          "to": [
            { "shell_command": "open -a 'safari'" }
          ],
          "conditions": [
            <%= variable_if(["left_control_key"], [1]) %>
          ]
        },
        {
          "type": "basic",
          "from": <%= from("left_control", [], ["any"]) %>,
          "to": [
            <%= set_variable(["left_control_key"], [1]) %>,
            { "key_code": "left_control"}
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              <%= set_variable(["left_control_key"], [0]) %>
            ],
            "to_if_canceled": [
              <%= set_variable(["left_control_key"], [0]) %>
            ]
          },
          "conditions": [
            <%= variable_if(["left_control_key"], [0]) %>
          ]
        }
      ]
    },
    {
      "description": "pointing_button in 'from'",
      "manipulators": [
        {
          "type": "basic",
          "from": { "pointing_button": "button3" },
          "to": <%= to([["t", ["command"]]]) %>,
          "conditions": [
            <%= frontmost_application_if("finder") %>
          ]
        }
      ]
    },
    {
      "description": "Test 'frontmost_application_if > file_paths'",
      "manipulators": [
       {
          "type": "basic",
          "from": {
            "pointing_button": "button1",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": <%= to([["t", ["command"]]]) %>,
          "conditions": [
            {
              "type": "frontmost_application_if",
              "file_paths": [
                "/System/Library/CoreServices/Finder.app/Contents/MacOS/Finder"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Escapeキー長押しでAlfred起動",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("escape", [], ["caps_lock"]) %>,
          "to_if_alone": <%= to(["escape"]) %>,
          "to_if_held_down": [
            { "shell_command": "open -a 'Alfred 3.app'" }
          ]
        }
      ]
    },
    {
      "description": "Qiita回答(2019/11/17)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_alt", [], ["any"]) %>,
          "to_if_alone": <%= to([["spacebar", ["command"]]]) %>,
          "to": [
            {
              "key_code": "left_option",
              "lazy": true
            }
          ]
        }
      ]
    },
    {
      "description": "マウステスト",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("7", ["left_option"], ["caps_lock"]) %>,
          "to": [
            { "mouse_key": {"x": -960} }
          ]
        }
      ]
    },
    {
      "description": "S,D,F同時押しでAlfred起動",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "s" },
              { "key_code": "d" },
              { "key_code": "f" }
            ]
          },
          "to": [
            { "shell_command": "open -a 'Alfred 3.app'" }
          ]
        }
      ]
    },
    {
      "description": "S,D同時押しで左矢印（detect_key_down_uninterruptedly=true）",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "s" },
              { "key_code": "d" }
            ],
            "simultaneous_options": {
              "detect_key_down_uninterruptedly": true
            }
          },
          "to": <%= to(["left_arrow"]) %>
        }
      ]
    },
    {
      "description": "F,R同時押しで上矢印（key_down_order=strict_inverse）",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "f" },
              { "key_code": "r" }
            ],
            "simultaneous_options": {
              "key_down_order": "strict_inverse"
            }
          },
          "to": <%= to(["up_arrow"]) %>
        }
      ]
    },
    {
      "description": "J,K同時押しで下矢印（key_up_order=strict_inverse）",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "j" },
              { "key_code": "k" }
            ],
            "simultaneous_options": {
              "key_up_order": "strict_inverse"
            }
          },
          "to": <%= to(["down_arrow"]) %>
        }
      ]
    },
    {
      "description": "J,K同時押しで下矢印（key_up_order=insensitive）",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "j" },
              { "key_code": "k" }
            ],
            "simultaneous_options": {
              "key_up_order": "insensitive"
            }
          },
          "to": <%= to(["down_arrow"]) %>
        }
      ]
    },
    {
      "description": "J,K同時押しで下矢印（key_up_order=strict）",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "j" },
              { "key_code": "k" }
            ],
            "simultaneous_options": {
              "key_up_order": "strict"
            }
          },
          "to": <%= to(["down_arrow"]) %>
        }
      ]
    },
    {
      "description": "テンキーの0,-同時押しでBackSpace（key_up_when=all）",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "keypad_0" },
              { "key_code": "keypad_hyphen" }
            ],
            "simultaneous_options": {
              "key_up_when": "all"
            }
          },
          "to": <%= to(["delete_or_backspace"]) %>
        }
      ]
    },
    {
      "description": "テンキーの0,-同時押しでBackSpace（key_up_when=any）",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "keypad_0" },
              { "key_code": "keypad_hyphen" }
            ],
            "simultaneous_options": {
              "key_up_when": "any"
            }
          },
          "to": <%= to(["delete_or_backspace"]) %>
        }
      ]
    },
    {
      "description": "テンキーの0,*同時押しでフラグを立てる",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "keypad_0" },
              { "key_code": "keypad_asterisk" }
            ],
            "simultaneous_options": {
              "to_after_key_up": [
                {
                  "set_variable": {
                    "name": "hogehoge",
                    "value": 0
                  }
                }
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "hogehoge",
                "value": 1
              }
            }
          ]
        }
      ]
    },
    {
      "description": "to_if_aloneとto_after_key_upの発火順を確認する",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_gui",[],["any"]) %>,
          "to":  <%= to(["left_gui"]) %>,
          "to_if_alone": <%= to(["japanese_eisuu"]) %>,
          "to_after_key_up": <%= to(["japanese_kana"]) %>
        }
      ]
    },
    {
      "description": "F1 -> ctrl+←",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f1", [],["any"]) %>,
          "to": <%= to([["left_arrow", ["control"]]]) %>
        }
      ]
    },
    {
      "description": "F2 -> ctrl+→",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f2", [],["any"]) %>,
          "to": <%= to([["right_arrow", ["control"]]]) %>
        }
      ]
    },
    {
      "description": "lang2 + j -> vk_launchpad",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("j", ["lang2"],[]) %>,
          "to": <%= to(["vk_launchpad"]) %>
        }
      ]
    },
    {
      "description": "Simultaneous Right & Left Click Open LaunchPad",
      "manipulators": [
        {
          "type": "basic",
          "parameters": {
            "basic.simultaneous_threshold_milliseconds": 500
          },
          "from": {
            "simultaneous": [
              { "pointing_button": "button1" },
              { "pointing_button": "button2" }
            ]
          },
          "to": <%= to(["vk_launchpad"]) %>
        }
      ]
    },
    {
      "description": "Simultaneous Right & Left Click to Command-N",
      "manipulators": [
        {
          "type": "basic",
          "parameters": {
            "basic.simultaneous_threshold_milliseconds": 50
          },
          "from": {
            "simultaneous": [
              { "pointing_button": "button1" },
              { "pointing_button": "button2" }
            ]
          },
          "to": <%= to([["n", ["command"]]]) %>
        }
      ]
    },
    {
      "description": "Cmd-Insert Open vk_launchpad",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("insert", ["command"],[]) %>,
          "to": <%= to(["vk_launchpad"]) %>
        }
      ]
    },
    {
      "description": "command-Enter to Double Click",
      "comment": "cmd-enterをダブルクリックにしようとしたがうまくいかなかった",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("return_or_enter", ["command"]) %>,
          "to": [
            { "pointing_button": "button1" },
            { "pointing_button": "button1" }
          ],
          "conditions": [
            <%= frontmost_application_if("finder") %>
          ]
        }
      ]
    },
    {
      "description": "fn to Double Click",
      "comment": "Macbook pro の fn キーを `from` に指定できるか確認",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("fn") %>,
          "to": [
            { "pointing_button": "button1" },
            { "pointing_button": "button1" }
          ]
        }
      ]
    },
    {
      "description": "Qiita回答2",
      "comment": "左右コマンド同時＋N -> ↓",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("n", ["right_command"], ["left_command", "option", "shift"]) %>,
          "to": <%= to(["down_arrow"]) %>
        }
      ]
    },
    {
      "description": "fn+H to left arrow",
      "comment": "Macbook pro の fn キーを `modifier` に指定できるか確認",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("h", ["fn"]) %>,
          "to": <%= to(["left_arrow"]) %>
        }
      ]
    },
    {
      "description": "test 'event_changed_if'",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("q", ["left_control", "left_command"]) %>,
          "to": <%= to([["f3"]]) %>,
          "conditions": [
            {
              "type": "event_changed_unless",
              "value": true
            }
          ]
        }
      ]
    }
  ]
}
